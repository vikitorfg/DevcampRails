<p><%= link_to portfolio_items.title, portfolio_show_path(portfolio_items)%></p>
<p><%= portfolio_items.subtitle%></p>
<p><%= portfolio_items.body%></p>
<%= image_tag portfolio_items.thumb_image unless portfolio_items.thumb_image.nil? %>
<%= link_to "Edit", edit_portfolio_path(portfolio_items) if logged_in?(:site_admin)%>
<%= link_to 'Delete', portfolio_path(portfolio_items), method: :delete, data: { confirm: 'Are you sure?' } if logged_in?(:site_admin) %>
